@model List<FrontEnd.Models.ShipperViewModel>

<h2>Shippers</h2>
<table>
    <tr>
        <th>Shipper ID</th>
        <th>Company Name</th>
        <th>Phone</th>
    </tr>
    @foreach (var shipper in Model)
    {
        <tr>
            <td>@shipper.ShipperId</td>
            <td>@shipper.CompanyName</td>
            <td>@shipper.Phone</td>
        </tr>
    }
</table>
